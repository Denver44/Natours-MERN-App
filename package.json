{
  "name": "travel-space",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "nodemon -r dotenv/config server.js dotenv_config_path=.env.local",
    "start:prod": "nodemon -r dotenv/config server.js server.js dotenv_config_path=.env.production",
    "import": "node -r dotenv/config  seeding/import-dev-data.js dotenv_config_path=.env.local import",
    "flush": "node -r dotenv/config  seeding/import-dev-data.js dotenv_config_path=.env.local delete",
    "check-lint": "eslint --fix --ext .js .",
    "check-format": "prettier --check .",
    "fix-format": "prettier --write .",
    "prepare": "husky install",
    "pre-commit": "yarn check-format && yarn fix-format && yarn check-lint && git add -A .",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/build --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/build --out-file bundle.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^6.1.0",
    "helmet": "^5.0.1",
    "hpp": "^0.2.3",
    "html-to-text": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.3.0",
    "mongoose": "^6.1.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.2",
    "pug": "^3.0.2",
    "sharp": "^0.29.3",
    "slugify": "^1.6.4",
    "stripe": "^8.197.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^2.5.1"
  }
}
